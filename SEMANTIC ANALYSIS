def semantic_analysis(ast, symbol_table):
    if ast[0] == 'NUMBER':
        return 'int'
    elif ast[0] == 'ID':
        if ast[1] not in symbol_table:
            raise Exception(f"Semantic Error: {ast[1]} not declared")
        return symbol_table[ast[1]]
    else:
        left_type = semantic_analysis(ast[1], symbol_table)
        right_type = semantic_analysis(ast[2], symbol_table)
        if left_type != right_type:
            raise Exception("Type Mismatch")
        return left_type
